<% provide :title, t(".page_title") %>

<div class="user-list">
  <table>
    <thead>
      <tr>
        <th></th>
        <th><%= t ".name" %></th>
        <th><%= t ".email" %></th>
        <th colspan="3"><%= t ".utility" %></th>
      </tr>
    </thead>
    <tbody>
      <%= render @users %>
    </tbody>
    <%= paginate @users, theme: "twitter-bootstrap-3" %>
  </table>
</div>
